   );\n    }\n    return arr;\n  };\n\n  return (\n    <>\n      <div ref={scrollRef} className={`scroll-container ${recog}`}>\n        <div className={`part-img ${levelText}`} style={style} alt='part' />\n      </div>\n      <div className='text'>{loremTest()}</div>\n    </>\n  );\n};\n\nexport default Scroll;\n","/Users/youngjunkimg/study/cra-test/src/pages/basketball/basket-components/top/index.js",[],"/Users/youngjunkimg/study/cra-test/src/pages/basketball/basket-components/mid/index.js",["200"],"/Users/youngjunkimg/study/cra-test/src/component/UnderText/UnderText.js",[],"/Users/youngjunkimg/study/cra-test/src/component/NewScroll/index.js",["201","202","203","204"],"/Users/youngjunkimg/study/cra-test/src/component/LogoHover/path.js",[],"/Users/youngjunkimg/study/cra-test/src/pages/basketball/basket-components/mid/score/index.js",[],"/Users/youngjunkimg/study/cra-test/src/component/UpTo/UpTo.js",["205","206","207","208","209"],"/Users/youngjunkimg/study/cra-test/src/redux/video/index.js",[],"/Users/youngjunkimg/study/cra-test/src/component/pagePercent/index.js",["210"],"/Users/youngjunkimg/study/cra-test/src/component/SoundBar/index.js",["211"],"/Users/youngjunkimg/study/cra-test/src/api/api.js",[],"/Users/youngjunkimg/study/cra-test/src/pages/htmlTest/index.js",[],{"ruleId":"212","severity":1,"message":"213","line":1,"column":17,"nodeType":"214","messageId":"215","endLine":1,"endColumn":26},{"ruleId":"212","severity":1,"message":"216","line":1,"column":28,"nodeType":"214","messageId":"215","endLine":1,"endColumn":36},{"ruleId":"212","severity":1,"message":"217","line":1,"column":38,"nodeType":"214","messageId":"215","endLine":1,"endColumn":44},{"ruleId":"218","replacedBy":"219"},{"ruleId":"220","replacedBy":"221"},{"ruleId":"212","severity":1,"message":"222","line":12,"column":8,"nodeType":"214","messageId":"215","endLine":12,"endColumn":14},{"ruleId":"218","replacedBy":"223"},{"ruleId":"220","replacedBy":"224"},{"ruleId":"225","severity":1,"message":"226","line